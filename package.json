{
  "name": "rock-with-me",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "heroku-prebuild": "cd client && yarn",
    "client": "cd client && npm start",
    "build": "cd client && npm run build && npm run build-storybook",
    "storybook": "cd client && npm run storybook",
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\" \"npm run storybook\"",
    "dev:server": "npm run build && cd .. && npm start",
    "server": "nodemon server.js",
    "start": "node server.js"
  },
  "repository": "git@github.com:nadinho/rock-with-me.git",
  "author": "nadinho <rockt2935@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^5.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "husky": "^4.2.3",
    "lint-staged": "^10.1.2",
    "nodemon": "^2.0.3",
    "prettier": "^2.0.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": "eslint --fix",
    "*.{js,jsx,ts,tsx,md,html,css}": "prettier --write"
  },
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "bcrypt": "^4.0.1",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^9.0.0",
    "mongodb": "^3.5.5",
    "mongoose": "^5.9.12",
    "react-query": "^1.3.3"
  }
}
